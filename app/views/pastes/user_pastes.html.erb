            <div class="panel panel-default">
              <div class="panel-body">
                <table class="table table-striped">
                  <thead>
                    <tr>
                      <th><%= "Title" %></th>
                      <th><%= "Created At" %></th>
                      <th><%= "Edit" %></th>
                      <th><%= "Delete" %></th>
                    </tr>
                  </thead>
                  <tbody>
                  <% @pastes.each do |paste| %>
                    <tr>
                      <td><%= paste.title %></td>
                      <td><%= paste.created_at.strftime('%-d %b %Y %T') %></td>
                      <td class="text-center"><%= editPasteLink paste %></td>
                      <td class="text-center"><%= deletePasteLink paste %></td>
                    </tr>
                  <% end %>
                  </tbody>
                </table>
              </div>
            </div>
